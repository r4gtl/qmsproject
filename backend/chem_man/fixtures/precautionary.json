[{"model": "chem_man.precautionarystatement", "pk": 1, "fields": {"codice": "P101", "descrizione": "In caso di consultazione di un medico, tenere a disposizione il contenitore o l’etichetta del prodotto.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 2, "fields": {"codice": "P102", "descrizione": "Tenere fuori dalla portata dei bambini.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 3, "fields": {"codice": "P103", "descrizione": "Leggere attentamente e seguire tutte le istruzioni.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 4, "fields": {"codice": "P201", "descrizione": "Procurarsi istruzioni specifiche prima dell’uso.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 5, "fields": {"codice": "P202", "descrizione": "Non manipolare prima di avere letto e compreso tutte le avvertenze.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 6, "fields": {"codice": "P210", "descrizione": "Tenere lontano da fonti di calore, superfici calde, scintille, fiamme libere o altre fonti di accensione. Non fumare.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 7, "fields": {"codice": "P211", "descrizione": "Non vaporizzare su una fiamma libera o altra fonte di accensione.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 8, "fields": {"codice": "P212", "descrizione": "Evitare di riscaldare sotto confinamento o di ridurre l’agente desensibilizzante.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 9, "fields": {"codice": "P220", "descrizione": "Tenere lontano da indumenti e altri materiali combustibili.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 10, "fields": {"codice": "P222", "descrizione": "Evitare il contatto con l’aria.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 11, "fields": {"codice": "P223", "descrizione": "Evitare qualunque contatto con l’acqua.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 12, "fields": {"codice": "P230", "descrizione": "Mantenere umido con….", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 13, "fields": {"codice": "P231", "descrizione": "Manipolare e conservare in atmosfera di gas inerte/…", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 14, "fields": {"codice": "P232", "descrizione": "Proteggere dall’umidità.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 15, "fields": {"codice": "P233", "descrizione": "Tenere il recipiente ben chiuso.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 16, "fields": {"codice": "P234", "descrizione": "Conservare soltanto nell’imballaggio originale.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 17, "fields": {"codice": "P235", "descrizione": "Conservare in luogo fresco.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 18, "fields": {"codice": "P240", "descrizione": "Mettere a terra e a massa il contenitore e il dispositivo ricevente.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 19, "fields": {"codice": "P241", "descrizione": "Utilizzare impianti [elettrici/di ventilazione/d’illuminazione/…] a prova di esplosione.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 20, "fields": {"codice": "P242", "descrizione": "Utilizzare solo utensili antiscintillamento.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 21, "fields": {"codice": "P243", "descrizione": "Prendere precauzioni contro le scariche elettrostatiche.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 22, "fields": {"codice": "P244", "descrizione": "Mantenere le valvole e i raccordi liberi da olio e grasso.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 23, "fields": {"codice": "P250", "descrizione": "Evitare le abrasioni/gli urti/gli attriti/… .", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 24, "fields": {"codice": "P251", "descrizione": "Non perforare né bruciare, neppure dopo l’uso.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 25, "fields": {"codice": "P260", "descrizione": "Non respirare la polvere/i fumi/i gas/la nebbia/i vapori/gli aerosol.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 26, "fields": {"codice": "P261", "descrizione": "Evitare di respirare la polvere/i fumi/i gas/la nebbia/i vapori/gli aerosol.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 27, "fields": {"codice": "P262", "descrizione": "Evitare il contatto con gli occhi, la pelle o gli indumenti.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 28, "fields": {"codice": "P263", "descrizione": "Evitare il contatto durante la gravidanza e l’allattamento.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 29, "fields": {"codice": "P264", "descrizione": "Lavare accuratamente … dopo l’uso.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 30, "fields": {"codice": "P270", "descrizione": "Non mangiare, né bere, né fumare durante l’uso.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 31, "fields": {"codice": "P271", "descrizione": "Utilizzare soltanto all’aperto o in luogo ben ventilato.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 32, "fields": {"codice": "P272", "descrizione": "Gli indumenti da lavoro contaminati non devono essere portati fuori dal luogo di lavoro.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 33, "fields": {"codice": "P273", "descrizione": "Non disperdere nell’ambiente.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 34, "fields": {"codice": "P280", "descrizione": "Indossare guanti/indumenti protettivi/proteggere gli occhi/proteggere il viso/proteggere l’udito/…", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 35, "fields": {"codice": "P282", "descrizione": "Utilizzare guanti termici e schermo facciale o protezione per gli occhi.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 36, "fields": {"codice": "P283", "descrizione": "Indossare indumenti completamente ignifughi o in tessuti ritardanti di fiamma.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 37, "fields": {"codice": "P284", "descrizione": "[Quando la ventilazione del locale è insufficiente] indossare un apparecchio di protezione respiratoria.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 38, "fields": {"codice": "P231 + P232", "descrizione": "Manipolare e conservare in atmosfera di gas inerte/…. Tenere al riparo dall’umidità.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 39, "fields": {"codice": "P301", "descrizione": "IN CASO DI INGESTIONE:", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 40, "fields": {"codice": "P302", "descrizione": "IN CASO DI CONTATTO CON LA PELLE:", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 41, "fields": {"codice": "P303", "descrizione": "IN CASO DI CONTATTO CON LA PELLE (o con i capelli):", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 42, "fields": {"codice": "P304", "descrizione": "IN CASO DI INALAZIONE:", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 43, "fields": {"codice": "P305", "descrizione": "IN CASO DI CONTATTO CON GLI OCCHI:", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 44, "fields": {"codice": "P306", "descrizione": "IN CASO DI CONTATTO CON GLI INDUMENTI:", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 45, "fields": {"codice": "P308", "descrizione": "IN CASO di esposizione o di possibile esposizione:", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 46, "fields": {"codice": "P310", "descrizione": "Contattare immediatamente un CENTRO ANTIVELENI/un medico…", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 47, "fields": {"codice": "P311", "descrizione": "Contattare un CENTRO ANTIVELENI/un medico/…", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 48, "fields": {"codice": "P312", "descrizione": "Contattare un CENTRO ANTIVELENI/un medico/…./in caso di malessere.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 49, "fields": {"codice": "P313", "descrizione": "Consultare un medico.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 50, "fields": {"codice": "P314", "descrizione": "In caso di malessere, consultare un medico.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 51, "fields": {"codice": "P315", "descrizione": "Consultare immediatamente un medico.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 52, "fields": {"codice": "P320", "descrizione": "Trattamento specifico urgente (vedere……. su questa etichetta).", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 53, "fields": {"codice": "P321", "descrizione": "Trattamento specifico (vedere …….su questa etichetta).", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 54, "fields": {"codice": "P330", "descrizione": "Sciacquare la bocca.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 55, "fields": {"codice": "P331", "descrizione": "NON provocare il vomito.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 56, "fields": {"codice": "P332", "descrizione": "In caso di irritazione della pelle:", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 57, "fields": {"codice": "P333", "descrizione": "In caso di irritazione o eruzione della pelle:", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 58, "fields": {"codice": "P334", "descrizione": "Immergere in acqua fredda [o avvolgere con un bendaggio umido].", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 59, "fields": {"codice": "P335", "descrizione": "Rimuovere le particelle depositate sulla pelle.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 60, "fields": {"codice": "P336", "descrizione": "Sgelare le parti congelate usando acqua tiepida. Non sfregare la parte interessata.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 61, "fields": {"codice": "P337", "descrizione": "Se l’irritazione degli occhi persiste:", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 62, "fields": {"codice": "P338", "descrizione": "Togliere le eventuali lenti a contatto se è agevole farlo. Continuare a sciacquare.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 63, "fields": {"codice": "P340", "descrizione": "Trasportare l’infortunato all’aria aperta e mantenerlo a riposo in posizione che favorisca la respirazione.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 64, "fields": {"codice": "P342", "descrizione": "In caso di sintomi respiratori:", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 65, "fields": {"codice": "P351", "descrizione": "Sciacquare accuratamente per parecchi minuti.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 66, "fields": {"codice": "P352", "descrizione": "Lavare abbondantemente con acqua/…", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 67, "fields": {"codice": "P353", "descrizione": "Sciacquare la pelle [o fare una doccia].", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 68, "fields": {"codice": "P360", "descrizione": "Sciacquare immediatamente e abbondantemente gli indumenti contaminati e la pelle prima di togliersi gli indumenti.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 69, "fields": {"codice": "P361", "descrizione": "Togliere immediatamente tutti gli indumenti contaminati.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 70, "fields": {"codice": "P362", "descrizione": "Togliere gli indumenti contaminati.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 71, "fields": {"codice": "P363", "descrizione": "Lavare gli indumenti contaminati prima di indossarli nuovamente.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 72, "fields": {"codice": "P364", "descrizione": "E lavarli prima di indossarli nuovamente.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 73, "fields": {"codice": "P370", "descrizione": "In caso di incendio:", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 74, "fields": {"codice": "P371", "descrizione": "In caso di incendio grave e di quantità rilevanti:", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 75, "fields": {"codice": "P372", "descrizione": "Rischio di esplosione.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 76, "fields": {"codice": "P373", "descrizione": "NON utilizzare mezzi estinguenti se l’incendio raggiunge materiali esplosivi.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 77, "fields": {"codice": "P375", "descrizione": "Rischio di esplosione. Utilizzare i mezzi estinguenti a grande distanza.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 78, "fields": {"codice": "P376", "descrizione": "Bloccare la perdita se non c’è pericolo.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 79, "fields": {"codice": "P377", "descrizione": "In caso d’incendio dovuto a perdita di gas, non estinguere a meno che non sia possibile bloccare la perdita senza pericolo.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 80, "fields": {"codice": "P378", "descrizione": "Utilizzare….per estinguere", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 81, "fields": {"codice": "P380", "descrizione": "Evacuare la zona.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 82, "fields": {"codice": "P381", "descrizione": "In caso di perdita, eliminare ogni fonte di accensione.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 83, "fields": {"codice": "P390", "descrizione": "Assorbire la fuoriuscita per evitare danni materiali.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 84, "fields": {"codice": "P391", "descrizione": "Raccogliere il materiale fuoriuscito.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 85, "fields": {"codice": "P301+P310", "descrizione": "IN CASO DI INGESTIONE: contattare immediatamente un CENTRO ANTIVELENI/un medico/…", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 86, "fields": {"codice": "P301+P312", "descrizione": "IN CASO DI INGESTIONE: in presenza di malessere, contattare un CENTRO ANTIVELENI/un medico/… .", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 87, "fields": {"codice": "P301+P330+P331", "descrizione": "IN CASO DI INGESTIONE: sciacquare la bocca. NON provocare il vomito.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 88, "fields": {"codice": "P302 + P334", "descrizione": "IN CASO DI CONTATTO CON LA PELLE: immergere in acqua fredda o avvolgere con un bendaggio umido.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 89, "fields": {"codice": "P302+P352", "descrizione": "IN CASO DI CONTATTO CON LA PELLE: lavare abbondantemente con acqua/…", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 90, "fields": {"codice": "P303+P361+P353", "descrizione": "IN CASO DI CONTATTO CON LA PELLE (o con i capelli): togliersi di dosso immediatamente tutti gli indumenti contaminati. Sciacquare la pelle [o fare una doccia].", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 91, "fields": {"codice": "P304+P340", "descrizione": "IN CASO DI INALAZIONE: trasportare l’infortunato all’aria aperta e mantenerlo a riposo in posizione che favorisca la respirazione.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 92, "fields": {"codice": "P305+P351+P338", "descrizione": "IN CASO DI CONTATTO CON GLI OCCHI: sciacquare accuratamente per parecchi minuti. Togliere le eventuali lenti a contatto se è agevole farlo. Continuare a sciacquare.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 93, "fields": {"codice": "P306+P360", "descrizione": "IN CASO DI CONTATTO CON GLI INDUMENTI: sciacquare immediatamente e abbondantemente gli indumenti contaminati e la pelle prima di togliersi gli indumenti.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 94, "fields": {"codice": "P308+P311", "descrizione": "In caso di esposizione o di possibile esposizione: contattare un CENTRO ANTIVELENI/un medico/…", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 95, "fields": {"codice": "P308+P313", "descrizione": "IN CASO di esposizione o di possibile esposizione, consultare un medico.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 96, "fields": {"codice": "P332+P313", "descrizione": "In caso di irritazione della pelle: consultare un medico.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 97, "fields": {"codice": "P333+P313", "descrizione": "In caso di irritazione o eruzione della pelle: consultare un medico.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 98, "fields": {"codice": "P337+P313", "descrizione": "Se l’irritazione degli occhi persiste, consultare un medico.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 99, "fields": {"codice": "P342+P311", "descrizione": "In caso di sintomi respiratori: contattare un CENTRO ANTIVELENI/un medico/…", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 100, "fields": {"codice": "P361+P364", "descrizione": "Togliere immediatamente tutti gli indumenti contaminati e lavarli prima di indossarli nuovamente.", "note": null, "created_by": null, "created_at": "2023-01-01T00:00:00Z"}}, {"model": "chem_man.precautionarystatement", "pk": 101, "fields": {"codice": "P362+P364", "descrizione": "Togliere tutti gli indumenti contaminati e lavarli prima di indossarli